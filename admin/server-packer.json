        "disk_type": "pd-ssd",
        "ssh_username": "core"
      }
    ],
    "provisioners": [
      {
        "type": "shell",
        "inline": [
          "sleep 10",
          "uname -a",
          "uptime"
        ]
      }
    ]
  }